      "source": [
        "# direction (0 = center, 1 = up, 2 = right, 3 = down, 4 = left)\n",
        "def direction_to_mod(src, target, step, step_thr = 20):\n",
        "    ds = target - src\n",
        "    dx = ds[0]\n",
        "    dy = ds[1]\n",
        "    if step > step_thr:\n",
        "        print('target pos', target) \n",
        "        print('src pos', src)\n",
        "        print('ds dy', dx, dy) \n",
        "\n",
        "    if dx == 0 and dy == 0:\n",
        "        if step > step_thr:\n",
        "            print('move center')\n",
        "        return 0\n",
        "    if abs(dx) > abs(dy):\n",
        "        if dx > 0:\n",
        "            if step > step_thr:\n",
        "                print('move right')\n",
        "            return 2 \n",
        "        else:\n",
        "            if step > step_thr:\n",
        "                print('move left')\n",
        "            return 4\n",
        "    else:\n",
        "        if dy > 0:\n",
        "            if step > step_thr:\n",
        "                print('move down')\n",
        "            return 3\n",
        "        else:\n",
        "            if step > step_thr:\n",
        "                print('move up')\n",
        "            return 1"
      ]
    },
